{
  "name": "cottage-tandoori-windows-printer",
  "version": "3.0.0",
  "description": "Windows-first printing helper for Cottage Tandoori - leverages Windows print spooler for reliable thermal printing",
  "main": "index.js",
  "bin": {
    "printer-helper": "./index.js"
  },
  "pkg": {
    "assets": [],
    "targets": [
      "node20-win-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "start": "node index.js",
    "build": "pkg package.json",
    "test": "node index.js --test",
    "install-service": "node install-service.js",
    "uninstall-service": "node uninstall-service.js"
  },
  "author": "Cottage Tandoori",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "pdf-lib": "^1.17.1",
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "keywords": [
    "windows",
    "printing",
    "pos",
    "thermal",
    "epson",
    "receipt"
